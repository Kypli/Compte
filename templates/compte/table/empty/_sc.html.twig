{% for month in 1..12 %}
	{% set done =
			current_year > year or (current_year == year and month < current_month)
				? 'done'
				: current_year == year and month == current_month
					? 'current'
					: ''
	%}
	<td class='{{ done }}' {{ modalGestion|raw }}>0</td>
	<td class='{{ done }} anticipe' {{ modalGestion|raw }}></td>
{% endfor %}
