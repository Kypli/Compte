<tr>
	<td colspan="28"></td>
</tr>

<tr>
	<td colspan="2"></td>
	{% for totaux in operations['totaux_mois'] %}
		<td>{{ totaux['reel'] is defined ? totaux['reel'] : '0' }}</td>
		<td>{{ totaux['anticipe'] is defined ? totaux['anticipe'] : '0' }}</td>
	{% endfor %}
	<td colspan="3" class="bck_black"></td>
</tr>

<tr>
	<td colspan="2"></td>
	{% for totaux in operations['totaux_mois'] %}
		<td colspan="2">{{ totaux['total'] }}</td>
	{% endfor %}
	<td>{{ operations['total_final'] }}</td>
	<td>{{ (operations['total_final'] / 12)|round(2, 'ceil')  }}</td>
	<td>{{ 100 }}</td>
</tr>